{
  "name": "logview",
  "version": "0.1.0",
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
