{
  "name": "logcollect",
  "version": "0.1.0",
  "devDependencies": {
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.0"
  },
  "scripts": {
    "prettify": "prettier --write  './*.{js,tsx,json,css,md}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "free-memory": "^1.0.0",
    "socket.io": "^2.1.1"
  }
}
